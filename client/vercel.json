{
    "routes": [
        {
            "src": "/[^.]+",
            "dest": "/"
        },
        {
            "src": "^/api/(.*)$",
            "dest": "https://adorable-achievement-production.up.railway.app/$1"
        }
    ],
    "header": [
        {
            "source": "^/api/(.*)$",
            "header": [
                {
                    "key": "Content-Type",
                    "value": "application/json"
                }
            ]
        }
    ]
}
