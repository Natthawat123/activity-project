{
    "routes": [
      {
        "src": "/[^.]+",
        "dest": "/"
      },
      {
        "src": "^/api/(.*)$",
        "dest": "https://adorable-achievement-production.up.railway.app/$1"
      }
    ],
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ]
      }
    ]
  }
  